{
    "name": "@kyori-mfv/mfext-config",
    "version": "0.1.0",
    "description": "Shared configurations for ESLint, Prettier, and TypeScript in MFExt projects",
    "type": "module",
    "main": "eslint.config.js",
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "exports": {
        "./eslint": "./eslint.config.js",
        "./eslint/node": "./eslint.config.js",
        "./eslint/browser": "./eslint.config.js",
        "./prettier": "./prettier.config.js",
        "./tsconfig": "./tsconfig.json"
    },
    "files": [
        "eslint.config.js",
        "prettier.config.js",
        "tsconfig.json",
        "README.md"
    ],
    "keywords": [
        "eslint",
        "prettier",
        "typescript",
        "config",
        "mfext"
