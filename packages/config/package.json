    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^9.0.0",
        "prettier": "^3.0.0",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "eslint": "^9.0.0",
        "prettier": "^3.0.0",
        "typescript": "^5.0.0"
    }
